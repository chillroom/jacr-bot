{
  "name": "betabot",
  "version": "1.4.3",
  "description": "Just a chillroom dubtrack bot",
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "main": "app.js",
  "private": true,
  "dependencies": {
    "dubapi": "^1.0.1",
    "htmlparser": "^1.7.7",
    "jethro": "^2.6.3",
    "kerberos": "0.0.17",
    "mongoose": "^4.0.3",
    "request": "^2.64.0",
    "restify": "^4.0.3",
    "soupselect": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "mocha-tap-reporter": "^0.1.3",
    "should": "^7.1.1",
    "supertest": "^1.1.0"
  },
  "scripts": {
    "version": "git add --all",
    "postversion": "git push",
    "test": "node ./bot/index.js",
    "ci-build": "node_modules/.bin/gulp",
    "ci-test": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -R mocha-tap-reporter",
    "ci-lint": "node_modules/.bin/eslint -f checkstyle bot/**/* > checkstyle-result.xml"
